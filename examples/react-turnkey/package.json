{
  "name": "react-turnkey",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "pnpm link ../../packages/core ../../packages/react && vite",
    "dev:host": "pnpm link ../../packages/core ../../packages/react && vite --host",
    "dev:backend": "tsx watch server/index.ts",
    "dev:fullstack": "concurrently \"pnpm dev:backend\" \"pnpm dev:local\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "vite",
    "install:local": "pnpm link ../../packages/core ../../packages/react",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@stablecoin.xyz/react": "workspace:*",
    "@turnkey/sdk-react": "^5.4.10",
    "@turnkey/sdk-server": "^4.12.0",
    "@turnkey/viem": "^0.14.12",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "viem": "^2.33.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^9.2.1",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}