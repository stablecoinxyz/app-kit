# SBC Account Abstraction Configuration
# Copy this file to .env.local and update with your values

# Required: Your SBC API key from https://dashboard.stablecoin.xyz
REACT_APP_SBC_API_KEY=your_sbc_api_key_here

# Optional: Use staging environment (recommended for development)
REACT_APP_SBC_STAGING=true

# Optional: Enable debug logging (recommended for development)
REACT_APP_SBC_DEBUG=true

# Optional: Custom RPC URL (if not using default)
# REACT_APP_CUSTOM_RPC_URL=https://your-custom-rpc.com

# Optional: BetterStack logging (for production)
# REACT_APP_BETTERSTACK_TOKEN=your_betterstack_token
